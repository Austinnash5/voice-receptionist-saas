{
  "name": "ai-voice-receptionist",
  "version": "1.0.0",
  "description": "Multi-tenant AI Voice Receptionist SaaS with Twilio",
  "main": "app.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc && npm run copy-views",
    "copy-views": "mkdir -p dist/views && cp -r views dist/ && mkdir -p dist/public && cp -r public dist/",
    "start": "node app.js",
    "migrate": "prisma migrate deploy",
    "migrate:dev": "prisma migrate dev",
    "db:seed": "ts-node prisma/seed.ts",
    "prisma:generate": "prisma generate"
  },
  "keywords": [
    "twilio",
    "voice",
    "ai",
    "receptionist",
    "saas"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.9.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "connect-redis": "^7.1.0",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.4.0",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "express-session": "^1.18.0",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "nodemailer": "^6.9.9",
    "openai": "^4.26.0",
    "redis": "^4.6.12",
    "twilio": "^4.23.0",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.6",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.17.10",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.11.5",
    "@types/nodemailer": "^6.4.14",
    "@types/uuid": "^9.0.7",
    "prisma": "^5.9.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
